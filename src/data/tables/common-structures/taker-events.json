{
  "Started": {
    "data": [
      {
        "parameter": "taker_coin",
        "type": "string",
        "required": true,
        "description": "the ticker of the taker coin"
      },
      {
        "parameter": "maker_coin",
        "type": "string",
        "required": true,
        "description": "the ticker of the maker coin"
      },
      {
        "parameter": "maker",
        "type": "string (hexadecimal)",
        "required": true,
        "description": "the p2p ID of maker node"
      },
      {
        "parameter": "lock_duration",
        "type": "number (integer)",
        "required": true,
        "description": "the lock duration of swap payments in seconds. The sender can refund the transaction when the lock duration is passed. The taker payment is locked for the lock duration. The maker payment is locked for lock duration * 2"
      },
      {
        "parameter": "maker_amount",
        "type": "string (numeric)",
        "required": true,
        "description": "the amount of coins to be swapped by maker"
      },
      {
        "parameter": "taker_amount",
        "type": "string (numeric)",
        "required": true,
        "description": "the amount of coins to be swapped by taker"
      },
      {
        "parameter": "maker_payment_confirmations",
        "type": "number (integer)",
        "required": true,
        "description": "the required number of blockchain confirmations for maker payment"
      },
      {
        "parameter": "maker_payment_requires_nota",
        "type": "bool",
        "required": false,
        "description": "whether dPoW notarization is required for maker payment; can be null; available since `beta-2.0.1738`"
      },
      {
        "parameter": "taker_payment_confirmations",
        "type": "number (integer)",
        "required": true,
        "description": "the required number of blockchain confirmations for taker payment"
      },
      {
        "parameter": "taker_payment_requires_nota",
        "type": "bool",
        "required": false,
        "description": "whether dPoW notarization is required for taker payment; can be null; available since `beta-2.0.1738`"
      },
      {
        "parameter": "taker_payment_lock",
        "type": "number (UTC timestamp in seconds)",
        "required": true,
        "description": "the taker payment is locked until this timestamp"
      },
      {
        "parameter": "uuid",
        "type": "string",
        "required": true,
        "description": "the swap uuid" 
      },
      {
        "parameter": "started_at",
        "type": "number (UTC timestamp in seconds)",
        "required": true,
        "description": "the timestamp at the start of the swap"
      },
      {
        "parameter": "maker_coin_start_block",
        "type": "number (integer)",
        "required": true,
        "description": "the maker coin block number at the start of the swap"
      },
      {
        "parameter": "taker_coin_start_block",
        "type": "number (integer)",
        "required": true,
        "description": "the taker coin block number at the start of the swap"
      }
    ],
    "variant": "compact"
  },
